{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "sourceMap": true,
        "module": "esnext",
        "target": "es5",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "downlevelIteration": true,
        "resolveJsonModule": true,
        "strict": true
    }
}